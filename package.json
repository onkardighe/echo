{
  "name": "echo",
  "displayName": "Echo - P2P Audio Call",
  "description": "Peer-to-peer audio calls between developers entirely inside VS Code.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "echo.startCall",
        "title": "Echo: Start Call"
      },
      {
        "command": "echo.joinCall",
        "title": "Echo: Join Call"
      },
      {
        "command": "echo.endCall",
        "title": "Echo: End Call"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./ && node build.js",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/node": "20.x",
    "@types/vscode": "^1.96.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "esbuild": "^0.27.2",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3"
  }
}

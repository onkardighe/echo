{
    "manifest_version": 3,
    "name": "Echo",
    "version": "1.0",
    "description": "P2P connect",
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "action": {
        "default_title": "Launch Echo"
    },
    "background": {
        "service_worker": "dist/background.js"
    },
    "side_panel": {
        "default_path": "dist/sidepanel.html"
    },
    "permissions": [
        "sidePanel"
    ]
}